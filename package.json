{
	"name": "toptal-blockchain",
	"version": "0.0.1",
	"author": "Ezeki Zibzibadze",
	"description": "Demo project for Toptal",
	"main": "dist/index.js",
	"types": "dist/index.d.js",
	"scripts": {
		"start": "NODE_PATH=./dist node dist",
		"dev": "nodemon",
		"dev:node2": "PORT=3001 ts-node -r tsconfig-paths/register ./src/index.ts",
		"dev:node3": "GRAPHQL_PORT=3002 WS_PORT=4002 ts-node -r tsconfig-paths/register ./src/index.ts",
		"test": "ts-node ./node_modules/jokio/src/_test",
		"build": "tsc"
	},
	"dependencies": {
		"@types/ws": "^4.0.1",
		"apollo-cache-inmemory": "^1.1.11",
		"apollo-client": "^2.2.7",
		"apollo-link-ws": "^1.0.7",
		"crypto-js": "^3.1.9-1",
		"dotenv": "^5.0.1",
		"graphql-tag": "^2.8.0",
		"jokio": "^0.2.1",
		"jokio-graphql": "0.5.4",
		"rollup-plugin-typescript": "^0.8.1",
		"tsconfig-paths": "^3.1.3",
		"ws": "^5.0.0"
	},
	"devDependencies": {
		"@types/jest": "22.1.0",
		"jest": "22.1.4",
		"nodemon": "1.14.11",
		"ts-jest": "22.0.1",
		"ts-node": "4.0.1",
		"typescript": "2.6.2"
	},
	"repository": {
		"type": "git",
		"url": ""
	},
	"publishConfig": {
		"access": "public"
	},
	"keywords": [
		"jok"
	],
	"license": "MIT",
	"jest": {
		"transform": {
			"^.+\\.tsx?$": "ts-jest"
		},
		"testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
		"moduleFileExtensions": [
			"ts",
			"tsx",
			"js",
			"jsx",
			"json",
			"node"
		]
	}
}
